URxvt.scrollBar: false
! URxvt.font:                xft:DejaVu Sans Mono:pixelsize=15
URxvt*font: xft:Ubuntu Mono derivative Powerline:pixelsize=15
! URxvt*font: xft:Sauce Code Pro Nerd Font
!URxvt*font:                 xft:Monaco for Powerline:regular:size=8
!URxvt*imFont:               xft:Monaco for Powerline:regular:size=8
!URxvt*boldFont:             xft:Monaco for Powerline:bold:size=8
!URxvt*italicFont:           xft:Monaco for Powerline:italic:size=8
!URxvt*boldItalicFont:       xft:Monaco for Powerline:bold:italic:size=8
!URxvt*scrollColor:                    #f8f8f2
!URxvt*highlightColor:                 #49483e
!URxvt*highlightTextColor:             #a59f85

! special
*.foreground:   #c0c5ce
*.background:   #2b303b
*.cursorColor:  #c0c5ce

! black
*.color0:       #2b303b
*.color8:       #65737e

! red
*.color1:       #bf616a
*.color9:       #bf616a

! green
*.color2:       #a3be8c
*.color10:      #a3be8c

! yellow
*.color3:       #ebcb8b
*.color11:      #ebcb8b

! blue
*.color4:       #8fa1b3
*.color12:      #8fa1b3

! magenta
*.color5:       #b48ead
*.color13:      #b48ead

! cyan
*.color6:       #96b5b4
*.color14:      #96b5b4

! white
*.color7:       #c0c5ce
*.color15:      #eff1f5


URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
